{
  "name": "compact-send-slack",
  "version": "1.0.0",
  "description": "MCP server for sending compact prompts and progress to Slack webhook",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "compact-slack": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "cli": "node dist/cli.js",
    "install-global": "npm install -g .",
    "uninstall-global": "npm uninstall -g compact-send-slack"
  },
  "keywords": ["mcp", "slack", "webhook", "claude"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "axios": "^1.6.0",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}